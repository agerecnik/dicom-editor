<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#272537" />
    <SolidColorBrush x:Key="ElementBackgroundBrush" Color="#353340" />
    <SolidColorBrush x:Key="InvalidElementBackgroundBrush" Color="#7a302b" />
    <SolidColorBrush x:Key="TextBrush" Color="DarkGray" />

    <SolidColorBrush x:Key="SelectedBackgroundBrush" Color="#FFC5CBF9" />
    <SolidColorBrush x:Key="SelectedForegroundBrush" Color="Black" />

    <SolidColorBrush x:Key="ProgressBackgroundBrush" Color="#1a1824" />
    <SolidColorBrush x:Key="ProgressDoneBrush" Color="#504c70" />

</ResourceDictionary>